  <wadl:resources base="http://localhost:8080/PatientFolderAPI">
    <wadl:resource path="/rest/patientlist">
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Given colon separated namespace:urno pairs, add those patients to current user's my patient list.]]>
        </wadl:doc>
        <wadl:request/>
        <wadl:response>
          <wadl:doc>
            <![CDATA[list of patients added]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="results"/>
          <wadl:representation mediaType="text/vnd.infomedix+xml" element="results"/>
          <wadl:representation mediaType="text/xml" element="results"/>
          <wadl:representation mediaType="application/vnd.infomedix+json"/>
          <wadl:representation mediaType="application/json"/>
          <wadl:representation mediaType="application/vnd.infomedix+xml" element="results"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="DELETE">
        <wadl:doc>
          <![CDATA[Given colon separated namespace:urno pairs, delete those patients from current user's my patient list.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="nsUrnos" style="query"/>
        </wadl:request>
      </wadl:method>
    </wadl:resource>

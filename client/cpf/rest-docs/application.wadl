    <wadl:resource path="/rest/search/patients">
      <wadl:method name="GET">
        <wadl:request>
          <wadl:param name="domain" style="query">
            <wadl:doc>
              <![CDATA[The namespace (domain) of the patient URNO.]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="familyname" style="query">
            <wadl:doc>
              <![CDATA[The family name of the patient(s).]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="givenname" style="query">
            <wadl:doc>
              <![CDATA[The given name of the patient(s).]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="dateofbirth" style="query">
            <wadl:doc>
              <![CDATA[The patient's date of birth.  We expect the format of the date to be yyyymmdd,
however wildcards are allowed, where a year, month and/or day is replaced with
a ?.
<p>
For example, '19791216', '1980?20', '1970??'.  We allow '???' but that
is the same as specifying no date.]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="sex" style="query">
            <wadl:doc>
              <![CDATA[The sex of the patient.  These values follow what is specified by the hospital's
PAS, although common values are:
<p>
M  Male
<br>
F  Female
<br>
I  Indeterminate
<br>
U  Unknown]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="start" style="query"/>
          <wadl:param name="size" style="query"/>
          <wadl:param name="sort" style="query"/>
          <wadl:param name="order" style="query"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[List of patientDTOs]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="results"/>
          <wadl:representation mediaType="text/vnd.infomedix+xml" element="results"/>
          <wadl:representation mediaType="text/xml" element="results"/>
          <wadl:representation mediaType="application/vnd.infomedix+json"/>
          <wadl:representation mediaType="application/json"/>
          <wadl:representation mediaType="application/vnd.infomedix+xml" element="results"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="POST">

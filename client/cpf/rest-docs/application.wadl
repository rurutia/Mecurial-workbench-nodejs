    <wadl:resource path="/rest/search/documents">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Search for documents from NON-EPISODAL and Diagnostics sections.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="patientId" style="query"/>
          <wadl:param name="patientNs" style="query">
            <wadl:doc>
              <![CDATA[patient URNO (Must be used together with 'patientExtId')]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="masterId" style="query">
            <wadl:doc>
              <![CDATA[master id of document]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="docNs" style="query">
            <wadl:doc>
              <![CDATA[document namespace]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="docSectionId" style="query">
            <wadl:doc>
              <![CDATA[document section id
<ul>
<li>
When absent, return documents from non-episodal sections excluding 'Diagnostics'
</li>
<li>
When 'Diagnostics' is specified, only return documents and results from 'Diagnostics' section
</li>
<li>
When episodal section is specified, it gets ignored
</li>
</ul>]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="docCode" style="query">
            <wadl:doc>
              <![CDATA[document code (Not supported for Result search)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="docDescr" style="query">
            <wadl:doc>
              <![CDATA[document description]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="docMediaType" style="query">
            <wadl:doc>
              <![CDATA[document media type (The accepted docDate format is 'yyyyMMdd')]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="docDate" style="query">
            <wadl:doc>
              <![CDATA[document date]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="docState" style="query">
            <wadl:doc>
              <![CDATA[document state (Not supported for Result search)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="episodeNs" style="query"/>
          <wadl:param name="episodeId" style="query"/>
          <wadl:param name="start" style="query"/>
          <wadl:param name="size" style="query"/>
          <wadl:param name="sort" style="query"/>
          <wadl:param name="order" style="query"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[List of DocumentDTO]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="results"/>
          <wadl:representation mediaType="text/vnd.infomedix+xml" element="results"/>
          <wadl:representation mediaType="text/xml" element="results"/>
          <wadl:representation mediaType="application/vnd.infomedix+json"/>
          <wadl:representation mediaType="application/json"/>
          <wadl:representation mediaType="application/vnd.infomedix+xml" element="results"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Search for documents from NON-EPISODAL and Diagnostics sections.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="start" style="query"/>
          <wadl:param name="size" style="query"/>
          <wadl:param name="sort" style="query"/>
          <wadl:param name="order" style="query"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[List of DocumentDTO]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="results"/>
